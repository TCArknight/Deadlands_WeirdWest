    <exprreq message="Spellcasting d6+ required"><![CDATA[#traitfound[skSpellcst] >= 3]]></exprreq>
    </thing>
  <thing id="edgDWWImpHighRoll" name="Improved High Roller" description="Your inveterate gambler draws two extra cards (total) when Dealing with the Devil." compset="Edge" summary="Draw two extra cards (total) anytime you Deal with the Devil.">
    <usesource source="DWW_Core"/>
    <tag group="MinRank" tag="2"/>
