  <thing id="wpDWWDynamite" name="Dynamite (1 stick)" description="Dynamite has a 1 in 4 chance of exploding if it takes 6 or more points of damage (including being caught in a separate explosion). Throwing dynamite uses Athletics; setting it uses Repair. Anything more than seven sticks of dynamite is usually set to explode rather than thrown.\n\n{b}Note: {/b}The number of sticks in a stack will determine the Damage, Range and Area of effect." compset="SpecWeap" stacking="merge">
    <fieldval field="wpFireRate" value="-"/>
    <fieldval field="grCost" value="3"/>
    <fieldval field="gearWeight" value=".5"/>
    <usesource source="DWW_Core"/>
    <tag group="WeaponType" tag="SpecExplos" name="Special: Explosives" abbrev="Special: Explosives"/>
    <tag group="Weapon" tag="HvyWeapon"/>
    <tag group="Weapon" tag="Thrown"/>
    <eval phase="Initialize" priority="3500"><![CDATA[var cnt as number
var radius as number
var addl as number
var area as string

cnt = field[stackQty].value
radius = 6

if (cnt <= 2) then
  field[wpDamage].text = "2d6"
  field[wpShort].value = 4
  field[wpMedium].value = 8
  field[wpLong].value = 16
  perform assign[Weapon.MedBurst]
elseif (cnt <= 4) then
  field[wpDamage].text = "3d6"
  field[wpShort].value = 3
  field[wpMedium].value = 6
  field[wpLong].value = 12
  perform assign[Weapon.LargeBurst]
elseif (cnt <= 6) then
  field[wpDamage].text = "4d6"
  field[wpShort].value = 2
  field[wpMedium].value = 4
  field[wpLong].value = 8
  area = radius & chr(34) & " Radius"
  field[wpSpecial].text &= area
else
  field[wpDamage].text = "4d6"
  field[wpDmgBonus].value = 2*(cnt-6)
  field[wpShort].value = 2
  field[wpMedium].value = 4
  field[wpLong].value = 8
  addl = ((cnt-6)/2)
  radius += round(addl,0,-1)
  area = radius & chr(34) & " Radius"
  field[wpSpecial].text &= area
  endif]]></eval>
    </thing>
  <thing id="wpDWWNitro1" name="Nitroglycerine (1 pint)" description="Nitroglycerine is a colorless, oily, and unstable liquid explosive. It’s illegal to transport in 1884 after several notorious accidents, but can be made and used locally. Each pint of nitro acts as one stick of dynamite, but automatically explodes if a bottle takes 4 points of damage.\n\n{b}Note: {/b}The number of pints in a stack will determine the Damage, Range and Area of effect." compset="SpecWeap" stacking="merge">
    <fieldval field="wpFireRate" value="-"/>
    <fieldval field="grCost" value="3"/>
    <fieldval field="gearWeight" value=".5"/>
    <usesource source="DWW_Core"/>
    <tag group="WeaponType" tag="SpecExplos" name="Special: Explosives" abbrev="Special: Explosives"/>
    <tag group="Weapon" tag="HvyWeapon"/>
    <tag group="Weapon" tag="Thrown"/>
    <eval phase="Initialize" priority="3500"><![CDATA[var cnt as number
var radius as number
var addl as number
var area as string

cnt = field[stackQty].value
radius = 6

if (cnt <= 2) then
  field[wpDamage].text = "2d6"
  field[wpShort].value = 4
  field[wpMedium].value = 8
  field[wpLong].value = 16
  perform assign[Weapon.MedBurst]
elseif (cnt <= 4) then
  field[wpDamage].text = "3d6"
  field[wpShort].value = 3
  field[wpMedium].value = 6
  field[wpLong].value = 12
  perform assign[Weapon.LargeBurst]
elseif (cnt <= 6) then
  field[wpDamage].text = "4d6"
  field[wpShort].value = 2
  field[wpMedium].value = 4
  field[wpLong].value = 8
  area = radius & chr(34) & " Radius"
  field[wpSpecial].text &= area
else
  field[wpDamage].text = "4d6"
  field[wpDmgBonus].value = 2*(cnt-6)
  field[wpShort].value = 2
  field[wpMedium].value = 4
  field[wpLong].value = 8
  addl = ((cnt-6)/2)
  radius += round(addl,0,-1)
  area = radius & chr(34) & " Radius"
  field[wpSpecial].text &= area
  endif]]></eval>
    </thing>
