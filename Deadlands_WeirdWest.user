    <tag group="EdgeType" tag="Agent"/>
    </thing>
  <thing id="edgDWWGrade3" name="Grade 3" description="Use this Edge if you’re making a Seasoned character, either because the Marshal started the campaign at a higher level or you took the Veteran o’ the Weird West Edge.\n\nYour Agent is already Grade 3. He’s been given his mnemomizer, may upgrade his Gatling pistol to a Gatling rifle or shotgun if he chooses, and has three Favors.\n\nThe Agent knows the basics of the Reckoning, who the four Servitors are (or were), what Harrowed and hucksters are, and that some critters are invulnerable until you find their specific weakness." compset="Edge">
    <tag group="EdgeType" tag="Agent"/>
    <tag group="MinRank" tag="1"/>
    <pickreq thing="edgDWWAgent"/>
    </thing>
  <thing id="edgDWW1000Faces" name="Man Of A Thousand Faces" description="This Edge helps Agents infiltrate their foes, given some supplies and the ability to study the enemy. The Agent adds +2 to his Performance rolls when trying to impersonate a general “type” of roughly similar build. There’s a −2 penalty for impersonating someone the target has heard of, a −4 for someone the target has seen before, or –6 for a specific individual or someone the target is familiar with. Use of this Edge requires clothing appropriate for the role. A disguise kit (see page 27) removes 2 points of penalties." compset="Edge" summary="+2 to his Performance rolls when trying to impersonate a general “type” of roughly similar build.">
    <tag group="EdgeType" tag="Agent"/>
    <tag group="MinRank" tag="1"/>
    <pickreq thing="edgDWWAgent"/>
    <exprreq message="Performance d8+ required."><![CDATA[#traitfound[skPerformance] >= 4]]></exprreq>
    </thing>
  <thing id="edgDWWTrueBelieve" name="True Believer" description="This holy roller seems to have the ear his deity. He gets a free reroll on Faith rolls." compset="Edge" summary="Free reroll on Faith rolls.">
    <usesource source="DWW_Core"/>
    <tag group="MinRank" tag="0"/>
    <tag group="EdgeType" tag="Blessed"/>
    <pickreq thing="edgDWWBlessed"/>
    <exprreq message="Faith d6+ required"><![CDATA[#traitfound[skFaith] >= 3]]></exprreq>
    <exprreq message="Spirit d10+ required."><![CDATA[#trait[attrSpi] >= 5]]></exprreq>
    </thing>
  <thing id="edgDWWSepBlessed" name="– Blessed Edges –" compset="Edge" isshowonly="yes">
    <usesource source="DWW_Core"/>
    <tag group="Helper" tag="ModHeEdge" name="ModHeEdge" abbrev="ModHeEdge"/>
    <tag group="Helper" tag="ModLeEdge" name="ModLeEdge" abbrev="ModLeEdge"/>
