  <thing id="wpDWWBayonet" name="Bayonet" description="{b}NOTE: {/b} Equipping the Bayonet to a Carbine or Rifle will change the stats to the mounted weapon version." compset="Melee">
    <fieldval field="grCost" value="5"/>
    <fieldval field="gearWeight" value="1"/>
    <usesource source="TimeModern" parent="TimePeriod" name="Modern"/>
    <usesource source="DWW_Core"/>
    <tag group="WeaponDie" tag="2" name="d4" abbrev="d4"/>
    <tag group="StrReqDie" tag="2"/>
    <tag group="WeaponType" tag="ModMelee" name="Modern Melee" abbrev="Modern Melee"/>
    <tag group="User" tag="CanMount" name="Can be mounted on another weapon"/>
    <eval phase="Initialize" priority="3500"><![CDATA[var isValid as number
isValid = gearholder.tagis[User.BayonetValid]

if (isValid <> 0) then
  field[livename].text = field[thingname].text & " (Mounted)"
  if (tagis[WeaponDie.?] <> 0) then
    var newdie as number
    newdie = tagvalue[WeaponDie.?] + 1
    perform delete[WeaponDie.?]
    perform assignstr["WeaponDie." & newdie]
    endif

  perform delete[StrReqDie.?]
  perform assign[Equipment.TwoHand]
  field[wpParry].value += 1
  field[wpReach].value += 1
  endif]]></eval>
    <child entity="LoadOut"></child>
    </thing>
  <thing id="wpDWWBallard72" name="Ballard &apos;72 (.56)" compset="Ranged">
