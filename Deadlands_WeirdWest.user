    <eval phase="PreTraits" priority="5000" index="2"><![CDATA[if (field[abilActive].value <> 0) then
  #traitbonus[trParry] += 1
  hero.childfound[wpUnarmed].field[wpDmgBonus].value -= 2
  endif]]></eval>
    <pickreq thing="edgDWWABChiMast"/>
    <exprreq message="Fighting d8+ required."><![CDATA[#traitfound[skFighting] >= 4]]></exprreq>
    <exprreq message="Spirit d6+ required."><![CDATA[#trait[attrSpi] >= 3]]></exprreq>
    </thing>
  <thing id="edgDWWCeleKF" name="Celestial Kung Fu" description="More options become available to a martial artist as she masters its mystical and physical aspects.\n\nA hero with this Edge may assume two styles of Superior Kung Fu at once, and use either option as she sees fit. She may change one or both as a free action once per turn as usual." compset="Edge" summary="May assume two styles of Superior Kung Fu at once, and use either option as she sees fit." uniqueness="useronce">
    <usesource source="DWW_Core"/>
    <tag group="MinRank" tag="2"/>
    <tag group="EdgeType" tag="ChiMaster" name="Chi Master"/>
    <tag group="User" tag="Activation"/>
    <exprreq message="Fighting d10+ required."><![CDATA[#traitfound[skFighting] >= 5]]></exprreq>
    <exprreq message="Spirit d8+ required."><![CDATA[#trait[attrSpi] >= 4]]></exprreq>
    <exprreq message="Superior Kung Fu required."><![CDATA[hero.tagis[Helper.SuperiorKungFu] <> 0]]></exprreq>
    </thing>
  <thing id="rpDWWLetDevilOut" name="Let the Devil Out" description="The Harrowed embraces the demon’s power, though it may cost him his soul. See {b}Dominion{/b} on page 60." compset="RacialProp">
    <usesource source="DWW_Core"/>
    </thing>
  <thing id="rpDWWUndead" name="Undead" description="+2 to Toughness and Spirit rolls to recover from Shaken, ignore additional damage from Called Shots except to the head, ignore 1 point of Wound penalties, doesn’t breathe, immune to disease and poison, don’t Bleed Out, and are only permanently slain by destroying the brain." compset="RacialProp" summary="+2 to Toughness and Spirit rolls to recover from Shaken, ignore additional damage from Called Shots except to the head, ignore 1 point of Wound penalties, doesn’t breathe, immune to disease and poison, don’t Bleed Out.">
    <usesource source="DWW_Core"/>
    <eval phase="PreTraits" priority="5000"><![CDATA[
      perform #traitadjust[trTough,+,2,"Undead"]]]>
      <before name="Calc trtFinal"/>
      </eval>
    <eval phase="Setup" priority="5000" index="2">herofield[acIgnWound].value += 1</eval>
    </thing>
  <thing id="edgDWWClaws" name="Claws" description="The hero gains sharp claws that cause Str+d6 damage in combat. The Harrowed can extend or retract the claws as a free action." compset="Edge" uniqueness="unique">
