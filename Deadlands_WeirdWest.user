    <fieldval field="grModsUsed" value="1"/>
    <usesource source="DWW_Core"/>
    <tag group="ModType" tag="Melee"/>
    <eval phase="Traits" priority="5000">perform container.parent.assign[Weapon.GhostSteel]</eval>
    <eval phase="Traits" priority="5000" index="2" name="Add Ghost Steel Effects">container.parent.field[wpDmgBonus].value += field[modDmgBoTx].text
container.parent.field[wpPiercing].value += 1
container.parent.field[grHardness].value += 5
field[grCost].value = container.parent.field[grCost].value * 4
      <before name="Derived trtFinal"/>
      <after name="Downgrade Weapon Die"/>
      </eval>
